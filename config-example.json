{
  "OPENAI_API_KEY": "sk-your-openai-key-here",
  "RATE_LIMITING": {
    "global": {
      "requests_per_minute": 800,
      "concurrent_limit": 40,
      "queue_size": 100
    },
    "per_user": {
      "requests_per_minute": 60,
      "concurrent_limit": 2,
      "queue_size": 10
    },
    "enabled": true,
    "metrics_enabled": true
  },
  "ALLOWLIST": {
    "enabled": true,
    "endpoints": [
      "/v1/chat/completions",
      "/v1/completions", 
      "/v1/embeddings",
      "/v1/audio/speech",
      "/v1/audio/transcriptions"
    ],
    "models": [
      "gpt-5-mini",
      "gpt-4o",
      "whisper-1",
      "text-embedding-3-small",
      "tts-1",
      "tts-1-hd"
    ],
    "default_model": "gpt-5-mini"
  }
}
